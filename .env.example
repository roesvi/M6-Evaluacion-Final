# Configuración de la aplicación
VITE_APP_NAME="Mi Tienda Online"
VITE_APP_DESCRIPTION="Tienda en línea desarrollada con Vue 3"
VITE_APP_VERSION=1.0.0

# Configuración de la API
VITE_API_URL=http://localhost:3000/api
VITE_API_TIMEOUT=10000

# Configuración de autenticación
VITE_AUTH_TOKEN_KEY=auth_token
VITE_AUTH_USER_KEY=auth_user

# Configuración de características
VITE_ENABLE_ANALYTICS=false
VITE_ANALYTICS_ID=UA-XXXXX-Y

# Modo de desarrollo (solo para desarrollo local)
DEV=true

# Nota: No incluyas información sensible en este archivo
# Crea un archivo .env local para sobrescribir estos valores
